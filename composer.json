{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
		"intervention/image": ">1",
		"facebook/php-sdk-v4": "~4.0",
		"tricki/laravel-notification": "@dev",
		"watson/validating": "0.10.*",
		"maatwebsite/excel": "1.*",
		"jaybizzle/crawler-detect" :"1.*",
		"pda/pheanstalk": "2.1.*",
		"barryvdh/laravel-cors": "0.2.x",
		"cmgmyr/messenger": "~1.0",
		"artdarek/pusherer": "^1.0"
	},
	"require-dev": {
		"league/factory-muffin": "~2.1",
		"doctrine/dbal": "~2.3",
		"darsain/laravel-console": "dev-master",
		"hamcrest/hamcrest-php": "^2.0@dev"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/models/Users",
			"app/controllers/Users",
			"app/models/Photos",
			"app/controllers/Photos",
			"app/controllers/Albums",
			"app/models/Albums"
		],
		"psr-4": {
            "lib\\":"app/lib",
            "modules\\":"app/modules",
            "subscriber\\":"app/subscriber"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
